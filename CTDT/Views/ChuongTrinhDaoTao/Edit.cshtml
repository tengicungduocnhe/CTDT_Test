                                        </div>
                                    </div>
                                    <div class="col-md-6 col-12">
                                        <div class="form-group">
                                            <label asp-for="NamBatDauTuyenSinh" class="control-label"></label>
                                            <input asp-for="NamBatDauTuyenSinh" class="form-control" />
