@model CTDT.Models.TbNgonNguGiangDay

@{
    ViewData["Title"] = "Delete";
}

<h1>Xóa</h1>

<h3>Bạn có chắc chắn muốn xóa không?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            ID Chương trình đào tạo
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdChuongTrinhDaoTaoNavigation.IdChuongTrinhDaoTao)
        </dd>
        <dt class = "col-sm-2">
            ID Ngôn ngữ
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdNgonNguNavigation.IdNgoaiNgu)
        </dd>
        <dt class = "col-sm-2">
            ID Trình độ ngôn ngữ đầu vào
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdTrinhDoNgonNguDauVaoNavigation.IdKhungNangLucNgoaiNgu)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdNgonNguGiangDay" />
        <input type="submit" value="Xóa" class="btn btn-danger" /> |
        <a asp-action="Index">Quay Lại Danh Sách</a>
    </form>
</div>
