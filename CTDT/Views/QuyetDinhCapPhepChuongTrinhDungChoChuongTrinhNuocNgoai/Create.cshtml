@model CTDT.Models.TbQuyetDinhCapPhepChuongTrinhDungChoChuongTrinhNuocNgoai

@{
    ViewData["Title"] = "Create";
}
<h1>Tạo Mới</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="IdQuyetDinhCapPhepChuongTrinhDungChoChuongTrinhNuocNgoai" class="control-label"></label>
                <input asp-for="IdQuyetDinhCapPhepChuongTrinhDungChoChuongTrinhNuocNgoai" class="form-control" />
                <span asp-validation-for="IdQuyetDinhCapPhepChuongTrinhDungChoChuongTrinhNuocNgoai" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdChuongTrinhDaoTao" class="control-label"></label>
                <select asp-for="IdChuongTrinhDaoTao" class ="form-control" asp-items="ViewBag.IdChuongTrinhDaoTao"></select>
            </div>
            <div class="form-group">
                <label asp-for="IdLoaiQuyetDinh" class="control-label"></label>
                <select asp-for="IdLoaiQuyetDinh" class ="form-control" asp-items="ViewBag.IdLoaiQuyetDinh"></select>
            </div>
            <div class="form-group">
                <label asp-for="SoQuyetDinh" class="control-label"></label>
                <input asp-for="SoQuyetDinh" class="form-control" />
                <span asp-validation-for="SoQuyetDinh" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NgayBanHanhQuyetDinh" class="control-label"></label>
                <input asp-for="NgayBanHanhQuyetDinh" class="form-control" />
                <span asp-validation-for="NgayBanHanhQuyetDinh" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdHinhThucDaoTao" class="control-label"></label>
                <select asp-for="IdHinhThucDaoTao" class ="form-control" asp-items="ViewBag.IdHinhThucDaoTao"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Quay Lại Danh Sách</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
